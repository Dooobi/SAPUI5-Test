<mvc:View controllerName="my.namespace.controller.Master" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic"
	xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:t="sap.ui.table" xmlns:custom="my.namespace.customcontrol">
	<semantic:MasterPage id="master" title="{i18n>masterTitle}">
		<semantic:subHeader>
			<Toolbar>
				<SearchField></SearchField>
			</Toolbar>
		</semantic:subHeader>
		<semantic:content>
			<List id="masterList" mode="SingleSelectMaster" selectionChange="navToDetail">
				<StandardListItem icon="sap-icon://action-settings" title="Test"/>
			</List>
		</semantic:content>
	</semantic:MasterPage>
</mvc:View>