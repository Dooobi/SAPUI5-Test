<mvc:View controllerName="my.namespace.controller.Main" xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic"
	xmlns:t="sap.ui.table" xmlns:custom="my.namespace.customcontrol" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<Page id="page" title="{i18n>mainTitle}" enableScrolling="false">
		<content>
			<HBox>
				<Button text="Load Model" press="_loadViewModel"/>
				<Button text="Create RadioButtons" press="_createRadioButtons"/>
				<Button text="Reset" press="_reset"/>
			</HBox>
			<HBox id="container"></HBox>
		</content>
	</Page>
</mvc:View>
